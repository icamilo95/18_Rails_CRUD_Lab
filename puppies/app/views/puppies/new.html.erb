<h1>Add new Puppie</h1>


<% if @puppie.errors.any? %>
   <% @puppie.errors.full_messages.each do |error|%>
   <p cl=ass="text-danger">
      <%= error%>
   </p>
   <% end %>
<% end %>



<%= form_for @puppie, url:"/puppies" do |f| %>
   <%= f.label :name%>
   <%= f.text_field :name%>

   <%= f.label :age%>
   <%= f.text_field :age%>

   <%= f.label :breed%>
   <%= f.text_field :breed%>

   <%= f.submit %>
<% end %>


